<section id="contact" class="segment">
    <div class="container">
        <div class="row">
            <div class="col-md-12 ">
                <div class="heading-section">
                    <div class="main-heading">
                        <span>Contact Us</span>

                    </div>
                    <div class="sub-heading">
                        <h3 class="sub-title">
                            Let's <strong>get in touch</strong> to discuss your needs
                        </h3>
                        <p>
                            We simply want to make sure you get what you need and our service is providing
                            the best bang for your buck. Aliquam lorem ante, dapibus in. A vel lacus sagittis amet odio sociis lorem nascetur,
                            arcu sit proin a vel porttitor risus a lundium lundium elementum habitasse.
                        </p>
                    </div>
                </div><!--heading-->
            </div>
        </div><!--heading row end-->
    </div>

    <div class="contct-info parallax">
        <div class="container">
            <div class="row">
                <div class="col-md-4">
                    <div class="contct-detail">
                        <h3><i class="fa fa-map-marker"></i>
                        </h3>
                        <p>
                            100 Some Street<br>
                            Morphett Vale<br>
                            Adelaide,5000
                        </p>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="contct-detail">
                        <h3><i class="fa fa-phone"></i>
                        </h3>
                        <p>
                            +61 1234 567 890<br>
                            +61 4500 123 456 <br/>
                            <a href="#">admin@jmncommunications.com.au </a>
                        </p>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="contct-detail">
                        <h3><i class="fa fa-clock-o"></i>
                        </h3>
                        <p>
                            10 Am to 7 PM<br>
                            Monday to Friday<br>

                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div><!--contact info-->

    <div class="contact-form-wrap">
        <div class="container">
            <form name="sentMessage" id="contactForm" method="post" novalidate>
                <div class="row">
                    <div class=" col-md-6 ">
                        <div class="row control-group">
                            <div class="form-group col-xs-12 controls">
                                <!-- <label>Name<span>*</span></label>-->
                                <input type="text" class="form-control" placeholder="Name*" id="name" required data-validation-required-message="Please enter your name.">
                                <p class="help-block"></p>
                            </div>
                        </div>

                    </div>

                    <div class=" col-md-6">
                        <div class="row control-group">
                            <div class="form-group col-xs-12 controls">
                                <!-- <label>Email Address<span>*</span></label>-->
                                <input type="email" class="form-control" placeholder="Email Address*" id="email" required data-validation-required-message="Please enter your email address.">
                                <p class="help-block"></p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row control-group">
                    <div class="form-group col-md-6  controls">
                        <!-- <label>Phone Number<span>*</span></label>-->
                        <input type="tel" class="form-control" placeholder="Phone Number" id="phone">
                        <p class="help-block"></p>
                    </div>
                    <div class="form-group col-md-6  controls">
                        <!-- <label>Website</label>-->
                        <input type="text" class="form-control" placeholder="Website Url" id="website">

                    </div>
                </div>
                <div class="row control-group">
                    <div class="form-group col-xs-12 controls">
                        <!-- <label>Message<span>*</span></label>-->
                        <textarea rows="8" class="form-control" placeholder="Message*" id="message" required data-validation-required-message="Please enter a message."></textarea>
                        <p class="help-block"></p>
                    </div>
                </div>
                <br>
                <div id="success"></div>
                <div class="row">
                    <div class="form-group col-xs-12">
                        <button type="submit" class="btn btn-theme-bg btn-lg">Send Your Message</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
</section><!--contact-->



